{
    "name": "achouqfi/cplanner",
    "type": "project",
    "description": "Cplanner â€“ SaaS analytics dashboard using Laravel, Inertia.js, Vue 3, Tailwind CSS, and Vite with SSR & localization support.",
    "keywords": [
        "laravel",
        "starter",
        "project",
        "packages",
        "configurations"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/achouqfi/Cplanner.git"
    },
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-language-switch": "^3.1",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "inertiajs/inertia-laravel": "^1.0",
        "jackiedo/dotenv-editor": "^1.1",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^11.9",
        "laravel/helpers": "^1.7",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.6",
        "mcamara/laravel-localization": "^2.2",
        "mvenghaus/filament-plugin-translatable-inline": "^3.0",
        "opcodesio/log-viewer": "^3.15",
        "outerweb/filament-translatable-fields": "^2.0",
        "propaganistas/laravel-phone": "^5.3",
        "rawilk/filament-password-input": "^2.0",
        "solution-forest/filament-translate-field": "^1.3",
        "spatie/laravel-activitylog": "^4.9",
        "spatie/laravel-backup": "^9.2",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-missing-page-redirector": "^2.10",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-tags": "^4.7",
        "spatie/laravel-translatable": "^6.8",
        "spatie/laravel-translation-loader": "^2.8",
        "stevebauman/location": "^7.5",
        "testmonitor/laravel-vue-i18n-generator": "^2.0",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.2",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
